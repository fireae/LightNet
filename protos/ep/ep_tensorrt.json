        {
            "optype": "batchnorm",
            "rules": [
                {
                    "cond": [],
                    "replace": ["bn2scale_wts_cpu bwc", "tensorrt trt"],
                    "details": [
                        "bwc.ins[src_mean] = self.ins[mean]",
                        "bwc.ins[src_var] = self.ins[var]",
                        "bwc.ins[src_scale] = self.ins[scale]",
                        "bwc.ins[src_offset] = self.ins[offset]",
