                        "trt.outs[dst$@] = self.outs[dst]",
                        "trt.params[dst$^_shape] = self.outs[dst].dims",
                        "trt.params[dst$^_dtype] = ${type(char *) tl_dtype_name(${rh self.outs[dst].dtype})}",
